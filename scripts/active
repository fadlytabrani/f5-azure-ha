SELFIPS="$(tmsh list net self one-line | grep 'clb' | cut -d' ' -f3)"
if [ ! -z "$SELFIPS" ]
then
    while read -r line; do
        logger -p local0.info "Enabling SSH for cloud LB monitoring on self ip: ${line}"
        tmsh modify net self ${line} allow-service add { tcp:22 }
    done <<< "$SELFIPS"
fi